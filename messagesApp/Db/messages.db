	field(FTN,  "CHAR")
	field(FTO,  "CHAR")
	field(FTP,  "CHAR")
	field(FTQ,  "CHAR")
	field(FTR,  "CHAR")
	field(FTS,  "CHAR")
	field(FTT,  "LONG")
	field(FTU,  "LONG")

	field(FTVA,  "CHAR")
	field(FTVB,  "CHAR")
	field(FTVC,  "CHAR")
	field(FTVD,  "CHAR")
	field(FTVE,  "CHAR")
	field(FTVF,  "CHAR")
	field(FTVG,  "CHAR")
	field(FTVH,  "CHAR")
	field(FTVI,  "CHAR")
	field(FTVJ,  "CHAR")
	field(FTVK,  "CHAR")
	field(FTVL,  "CHAR")
	field(FTVM,  "CHAR")
	field(FTVN,  "CHAR")
	field(FTVO,  "CHAR")
	field(FTVP,  "CHAR")
	field(FTVQ,  "CHAR")
	field(FTVR,  "CHAR")
	field(FTVS,  "CHAR")
	field(FTVT,  "LONG")
	field(FTVU,  "LONG")

	field(SNAM, "messages")
	field(FLNK, "$(Sys)$(Dev)FOut_")
}

record(fanout, "$(Sys)$(Dev)FOut_"){
	field(LNK1, "$(Sys)$(Dev)Sts:Mail-Sts")
	field(LNK2, "$(Sys)$(Dev)FOut1_")
	field(LNK3, "$(Sys)$(Dev)FOut2_")
	field(LNK4, "$(Sys)$(Dev)Err-Sts")
}

record(calc, "$(Sys)$(Dev)Calc_"){
	field(INPA, "$(Sys)$(Dev)Sts:Mail-Sts NPP MS")
	field(INPB, "$(Sys)$(Dev)Mode-Sel NPP MS")
	field(CALC, "A==B")
}

record(bi, "$(Sys)$(Dev)Err-Sts"){
	field(INP, "$(Sys)$(Dev)Calc_ PP MS")
	field(ZNAM, "ERROR")
	field(ONAM, "OK")
	field(ZSRV, "MAJOR")
	field(OSRV, "NO_ALARM")
}

