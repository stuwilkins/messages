	field(OUTF, "$(Sys)$(Dev)Val:Msg6-RB NPP MS")
	field(NOVF,  "128")
	field(OUTG, "$(Sys)$(Dev)Val:Msg7-RB NPP MS")
	field(NOVG,  "128")
	field(OUTH, "$(Sys)$(Dev)Val:Msg8-RB NPP MS")
	field(NOVH,  "128")
	field(OUTI, "$(Sys)$(Dev)Val:Msg9-RB NPP MS")
	field(NOVI,  "128")
	field(OUTJ, "$(Sys)$(Dev)Val:Msg10-RB NPP MS")
	field(NOVJ,  "128")

	# Input Flags (Reserved)

	field(INPU, "$(Sys)$(Dev)Mode-Sel NPP MS")
	field(NOU,  "1")

	# Type setup

	field(FTA,  "CHAR")
	field(FTB,  "CHAR")
