record(aSub, "$(Sys)$(Dev)aSub"){
	field(INPA, "$(Sys)$(Dev)Val:Msg1-SP NPP MS")
	field(NOA,  "128")
	field(INPB, "$(Sys)$(Dev)Val:Msg2-SP NPP MS")
	field(NOB,  "128")
